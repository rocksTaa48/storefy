<%= form_with model: @subcategory do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  </br>
  <%= f.label :body %>
  <%= f.text_field :body %>
  </bf>
  <%= f.select "category_id", options_for_select(@categories.collect{|x| [x.title, x.id]}) %>
  </br>
  <%= f.submit "SAVE" %>
<% end %>