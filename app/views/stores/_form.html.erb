<%= form_with model: @store do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>
  <%= f.label :body %>
  <%= f.text_field :body %>
  <br/>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br/>
  <%= f.label :phone %>
  <%= f.phone_field :phone %>
  <br/>
  <%= f.submit "Done" %>
<% end %>